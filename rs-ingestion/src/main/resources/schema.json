{
  "definitions": {},
  "schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The PARCELAS_AGRICOLAS_MORENA_IGOR Schema",
  "required": [
    "_id",
    "program",
    "platform",
    "productType",
    "provider",
    "author",
    "title",
    "custom",
    "footprint",
    "acquisitionDateStart",
    "acquisitionDayOfYearStart",
    "acquisitionDateEnd",
    "acquisitionDayOfYearEnd",
    "frameworkMatrix",
    "accuracyPerColumn",
    "accuracyPerLine",
    "parcelsAverageArea",
    "parcelsAveragePerimeter",
    "parcelsCount",
    "processedProducts",
    "data"
  ],
  "properties": {
    "_id": {
      "$id": "#/properties/_id",
      "type": "string",
      "title": "Product id",
      "default": "",
      "examples": [
        "PA_MORENA_2015_LEZIRIATEJO"
      ],
      "pattern": "^(.*)$"
    },
    "program": {
      "$id": "#/properties/program",
      "type": "string",
      "title": "The Program that generated this product",
      "default": "",
      "enum": [
        "MORENA"
      ],
      "examples": [
        "MORENA"
      ],
      "pattern": "^(.*)$"
    },
    "platform": {
      "$id": "#/properties/platform",
      "type": "string",
      "title": "The platform where this product was acquired or generated",
      "default": "",
      "enum": [
        "Igor Ruivo"
      ],
      "examples": [
        "Igor Ruivo"
      ],
      "pattern": "^(.*)$"
    },
    "productType": {
      "$id": "#/properties/productType",
      "type": "string",
      "title": "Product type id",
      "default": "",
      "enum": [
        "PARCELAS_AGRICOLAS_MORENA_IGOR"
      ],
      "examples": [
        "PARCELAS_AGRICOLAS_MORENA_IGOR"
      ],
      "pattern": "^(.*)$"
    },
    "provider": {
      "$id": "#/properties/provider",
      "type": "string",
      "title": "Product provider",
      "default": "",
      "enum": [
        "DI@FCT-UNL"
      ],
      "examples": [
        "DI@FCT-UNL"
      ],
      "pattern": "^(.*)$"
    },
    "title": {
      "$id": "#/properties/title",
      "type": "string",
      "title": "Product name",
      "default": "",
      "examples": [
        "Parcelas Agrícolas da região da Lezíria do Tejo de 2015 computadas por Igor Ruivo no programa MORENA"
      ],
      "pattern": "^(.*)$"
    },
    "author": {
      "$id": "#/properties/author",
      "type": "string",
      "title": "Author of the product",
      "default": "",
      "enum": [
        "Igor Ruivo"
      ],
      "examples": [
        "Igor Ruivo"
      ],
      "pattern": "^(.*)$"
    },
    "custom": {
      "$id": "#/properties/custom",
      "type": "object",
      "title": "Custom metadata by users",
      "required": [],
      "properties": {}
    },
    "footprint": {
      "$id": "#/properties/footprint",
      "type": "object",
      "title": "Product footprint",
      "required": [
        "type",
        "coordinates"
      ],
      "properties": {
        "type": {
          "$id": "#/properties/footprint/properties/type",
          "type": "string",
          "title": "The Type Schema",
          "default": "",
          "examples": [
            "Polygon"
          ],
          "pattern": "^(.*)$"
        },
        "coordinates": {
          "$id": "#/properties/footprint/properties/coordinates",
          "type": "array",
          "title": "The Coordinates Schema"
        }
      }
    },
    "acquisitionDateStart": {
      "$id": "#/properties/acquisitionDateStart",
      "type": "string",
      "title": "Starting date of the product acquisition",
      "default": "",
      "examples": [
        "2019-07-10T23:36:16.318Z"
      ],
      "pattern": "^(.*)$"
    },
    "acquisitionDayOfYearStart": {
      "$id": "#/properties/acquisitionDayOfYearStart",
      "type": "string",
      "title": "Starting day of the year of the product acquisition",
      "default": "",
      "examples": [
        "192"
      ],
      "pattern": "^(.*)$"
    },
    "acquisitionDateEnd": {
      "$id": "#/properties/acquisitionDateEnd",
      "type": "string",
      "title": "Ending date of the product acquisition",
      "default": "",
      "examples": [
        "2019-07-10T23:36:48.088Z"
      ],
      "pattern": "^(.*)$"
    },
    "acquisitionDayOfYearEnd": {
      "$id": "#/properties/acquisitionDayOfYearEnd",
      "type": "string",
      "title": "Ending day of the year of the product acquisition",
      "default": "",
      "examples": [
        "192"
      ],
      "pattern": "^(.*)$"
    },
    "frameworkMatrix": {
      "$id": "#/properties/frameworkMatrix",
      "type": "array",
      "title": "Framework matrix from where it is derived the product accuracy",
      "items": {
        "$id": "#/properties/frameworkMatrix/items",
        "type": "array",
        "title": "The Items Schema"
      }
    },
    "accuracyPerColumn": {
      "$id": "#/properties/accuracyPerColumn",
      "type": "number",
      "title": "",
      "default": "",
      "examples": [
        "10.5"
      ]
    },
    "accuracyPerLine": {
      "$id": "#/properties/accuracyPerLine",
      "type": "number",
      "title": "",
      "default": "",
      "examples": [
        "10.5"
      ]
    },
    "parcelsAverageArea": {
      "$id": "#/properties/parcelsAverageArea",
      "type": "number",
      "title": "Agricultural parcels average area in km2",
      "default": "",
      "examples": [
        "10.5"
      ]
    },
    "parcelsAveragePerimeter": {
      "$id": "#/properties/parcelsAveragePerimeter",
      "type": "number",
      "title": "Agricultural parcels average perimeter in km2",
      "default": "",
      "examples": [
        "10.5"
      ]
    },
    "parcelsCount": {
      "$id": "#/properties/parcelsCount",
      "type": "integer",
      "title": "Number of agricultural parcels in the product area",
      "default": "",
      "examples": [
        "100"
      ]
    },
    "processedProducts": {
      "$id": "#/properties/processedProducts",
      "type": "array",
      "title": "Framework matrix from where it is derived the product accuracy",
      "required": [
        "bands",
        "dates",
        "granuleid"
      ],
      "properties": {
        "bands": {
          "$id": "#/properties/processedProducts/properties/bands",
          "type": "array",
          "title": "Product bands processed",
          "items": {
            "$id": "#/properties/processedProducts/properties/bands/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "1",
              "2"
            ],
            "pattern": "^(.*)$"
          }
        },
        "dates": {
          "$id": "#/properties/processedProducts/properties/dates",
          "type": "array",
          "title": "Granule dates of the products processed",
          "items": {
            "$id": "#/properties/processedProducts/properties/dates/items",
            "type": "string",
            "title": "The Items Schema",
            "default": "",
            "examples": [
              "2019-07-23T01:17:39.024Z"
            ],
            "pattern": "^(.*)$"
          }
        },
        "granuleid": {
          "$id": "#/properties/processedProducts/properties/granuleid",
          "type": "string",
          "title": "Granule identifier by US MGRS",
          "default": "",
          "examples": [
            "53NMJ"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "Data objects location",
      "required": [
        "imagery",
        "metadata"
      ],
      "properties": {
        "imagery": {
          "$id": "#/properties/data/properties/imagery",
          "type": "array",
          "title": "Product data location",
          "items": {
            "$id": "#/properties/data/properties/imagery/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "_id",
              "description",
              "url",
              "resolution",
              "format",
              "status",
              "fileName"
            ],
            "properties": {
              "_id": {
                "$id": "#/properties/data/properties/imagery/items/properties/_id",
                "type": "string",
                "title": "Data object id",
                "default": "",
                "examples": [
                  "23053239-7068-4a37-8033-bd125d1cb30d"
                ],
                "pattern": "^(.*)$"
              },
              "description": {
                "$id": "#/properties/data/properties/imagery/items/properties/description",
                "type": "string",
                "title": "Data object description",
                "default": "",
                "examples": [
                  "Shapefile with all the agricultural parcels in this area"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/data/properties/imagery/items/properties/url",
                "type": "string",
                "title": "Data object URL",
                "default": "",
                "examples": [
                  "./data/PARCELAS_AGRICOLAS_MORENA_IGOR/ZIP_PATH_FOR_FILE"
                ],
                "pattern": "^(.*)$"
              },
              "resolution": {
                "$id": "#/properties/data/properties/imagery/items/properties/resolution",
                "type": "object",
                "title": "Spatial resolution",
                "required": [
                  "x",
                  "y"
                ],
                "properties": {
                  "x": {
                    "$id": "#/properties/data/properties/imagery/items/properties/resolution/properties/x",
                    "type": "integer",
                    "title": "The X Schema",
                    "default": 0,
                    "examples": [
                      30
                    ]
                  },
                  "y": {
                    "$id": "#/properties/data/properties/imagery/items/properties/resolution/properties/y",
                    "type": "integer",
                    "title": "The Y Schema",
                    "default": 0,
                    "examples": [
                      30
                    ]
                  }
                }
              },
              "status": {
                "$id": "#/properties/data/properties/imagery/items/properties/status",
                "type": "string",
                "title": "Data Object status",
                "default": "",
                "examples": [
                  "remote",
                  "pending",
                  "local"
                ],
                "pattern": "^(.*)$"
              },
              "format": {
                "$id": "#/properties/data/properties/imagery/items/properties/format",
                "type": "string",
                "title": "Data Object format",
                "default": "",
                "examples": [
                  "tif",
                  "shp"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "metadata": {
          "$id": "#/properties/data/properties/metadata",
          "type": "array",
          "title": "Product metadata location",
          "items": {
            "$id": "#/properties/data/properties/metadata/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "_id",
              "description",
              "url",
              "status",
              "fileName"
            ],
            "properties": {
              "_id": {
                "$id": "#/properties/data/properties/imagery/items/properties/_id",
                "type": "string",
                "title": "Data object id",
                "default": "",
                "examples": [
                  "23053239-7068-4a37-8033-bd125d1cb30d"
                ],
                "pattern": "^(.*)$"
              },
              "description": {
                "$id": "#/properties/data/properties/imagery/items/properties/description",
                "type": "string",
                "title": "Data object description",
                "default": "",
                "examples": [
                  "Algorithm accuracy plot"
                ],
                "pattern": "^(.*)$"
              },
              "url": {
                "$id": "#/properties/data/properties/imagery/items/properties/url",
                "type": "string",
                "title": "Data object URL",
                "default": "",
                "examples": [
                  "./data/PARCELAS_AGRICOLAS_MORENA_IGOR/ZIP_PATH_FOR_FILE"
                ],
                "pattern": "^(.*)$"
              },
              "status": {
                "$id": "#/properties/data/properties/imagery/items/properties/status",
                "type": "string",
                "title": "Data Object status",
                "default": "",
                "examples": [
                  "remote",
                  "pending",
                  "local"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}